<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0" id="com.phonegap.applewatchcatalogue" versionCode="6" version="0.1.0.6">

    <!-- versionCode is optional and Android only -->

    <name>Apple Watch</name>

    <description>
      All Apple Watch models to view and choose your favorite. 
    </description>

    <author href="http://fedegarlo.com" email="fgarcialorca@gmail.com">
      Fede Garc√≠a
    </author>
    
    <gap:platform name="android" />
    <!--gap:platform name="winphone" /-->
    
    <icon src="res/icons/icon.png" />
    <icon gap:density="ldpi" gap:platform="android" src="res/icons/drawable-ldpi/ic_launcher.png" />
    <icon gap:density="mdpi" gap:platform="android" src="res/icons/drawable-mdpi/ic_launcher.png" />
    <icon gap:density="hdpi" gap:platform="android" src="res/icons/drawable-hdpi/ic_launcher.png" />
    <icon gap:density="xdpi" gap:platform="android" src="res/icons/drawable-xdpi/ic_launcher.png" />
    <icon gap:density="xxdpi" gap:platform="android" src="res/icons/drawable-xxdpi/ic_launcher.png" />
    <icon gap:density="xxxdpi" gap:platform="android" src="res/icons/drawable-xxxdpi/ic_launcher.png" />


    <gap:splash src="res/screen-hdpi-portrait.png" />
    <gap:splash gap:density="ldpi" gap:platform="android" src="res/screen-ldpi-portrait.png" />
    <gap:splash gap:density="mdpi" gap:platform="android" src="res/screen-mdpi-portrait.png" />
    <gap:splash gap:density="hdpi" gap:platform="android" src="res/screen-hdpi-portrait.png" />
    <gap:splash gap:density="xhdpi" gap:platform="android" src="res/screen-xhdpi-portrait.png" />
    <!--gap:splash src="splash.png" gap:platform="winphone" /-->

    
    
    <preference name="permissions" value="none"/>
    <preference name="android-minSdkVersion" value="9" />
    <preference name="fullscreen" value="true" />
    <preference name="targetSdkVersion" value="14" />
    <preference name="minSdkVersion" value="11" />
    <preference name="target-device" value="universal" />
    <preference name="BackupWebStorage" value="cloud" />
    <preference name="orientation" value="default" />
    <preference name="android-installLocation" value="auto" />
    <preference name="splash-screen-duration" value="1000" />
    <preference name="KeepRunning" value="true"/>
	<preference name="DisallowOverscroll" value="true"/>
    <preference name="phonegap-version" value="3.6.3" />
    
    
    <feature name="http://api.phonegap.com/1.0/file"/>
	<feature name="http://api.phonegap.com/1.0/geolocation"/>
	<feature name="http://api.phonegap.com/1.0/network"/>

    
    <!--<access origin="http://digitalpapyrus.eu" subdomains="true" />
    <access origin="http://digitalpapyrus.es" subdomains="true" />
    <access origin="http://outbook.es" subdomains="true" />
	<access origin="http://openstreetmap.org" subdomains="true" />
    <access origin="http://stamen.com" subdomains="true" />
    <access origin="http://openlayers.org" subdomains="true" />
    <access origin="openweathermap.org" subdomains="true" />-->

</widget>
